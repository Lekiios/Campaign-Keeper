{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "target": "es2020",
    "outDir": "./dist",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "include": ["src/**/*.ts"],
  "exclude": [".eslintrc.json", "node_modules"]
}
